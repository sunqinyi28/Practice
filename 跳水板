    public int[] divingBoard(int shorter, int longer, int k) {
        if(k==0)return new int[]{};
        if(shorter==longer)return new int[]{shorter*k};
        int[] result = new int[k+1];
        for(int i=0;i<k+1;i++){
            result[i]=longer*i+(k-i)*shorter;
        }
        return result;
    }
